   issn = {1553-7358},
   issue = {12},
   journal = {PLoS computational biology},
   keywords = {Bacteria,Bacteria: genetics,Bacteria: isolation & purification,Genes, Bacterial,Host-Pathogen Interactions,Humans,Metagenome,Metagenomics,RNA, Ribosomal, 16S,RNA, Ribosomal, 16S: genetics},
