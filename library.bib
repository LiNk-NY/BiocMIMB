   keywords = {Animals,Bias (Epidemiology),Chromosome Mapping,Chromosome Mapping: methods,Computer Simulation,Databases, Genetic,Gene Expression Regulation,Hematopoietic Stem Cells,Hematopoietic Stem Cells: metabolism,Hippocampus,Hippocampus: metabolism,Lung,Lung: metabolism,Mice,Models, Genetic,Mutation,Mutation: genetics,Polymorphism, Single Nucleotide,Polymorphism, Single Nucleotide: genetics,Quantitative Trait Loci,Quantitative Trait Loci: genetics,Quantitative Trait, Heritable,Sample Size,T-Lymphocytes, Regulatory,T-Lymphocytes, Regulatory: metabolism},
   month = {1},
