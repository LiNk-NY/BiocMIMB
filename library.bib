   journal = {Cell},
   keywords = {Breast Neoplasms,Breast Neoplasms: genetics,Cell Line, Tumor,Gene Expression Profiling,Genetic Predisposition to Disease,Genome-Wide Association Study,Humans,Quantitative Trait Loci},
   month = {1},
