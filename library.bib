   keywords = {B-Lymphocytes,B-Lymphocytes: cytology,B-Lymphocytes: immunology,Cell Differentiation,Gene Expression Profiling,Humans,Immunologic Memory,Lymphoma,Lymphoma: genetics,Lymphoma: pathology},
   month = {2},
   pages = {2672-7},
   pmid = {22308355},
