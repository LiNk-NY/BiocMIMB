@article{ABCDEFG,
   abstract = {Cancer treatment options have evolved significantly in the past few years. From the initial surgical procedures, to the latest next-generation technologies, we are now in the position to analyze and understand tumors in a one-by-one basis and use that to our advantage to provide with individualized treatment options that may increase patient survival. In this review, we will focus on how tumor profiling has evolved over the past decades to deliver more efficient and personalized treatment options, and how novel technologies can help us envisage the future of precision oncology toward a better management and, ultimately, increased survival.},
   author = {Ceres Fernandez-Rozadilla and Ana Rita Simões and Matilde E. Lleonart and Amancio Carnero and Ángel Carracedo},
