   month = {9},
   note = {NULL},
   pages = {2098-103},
   pmid = {20805282},
   title = {Disposition index, glucose effectiveness, and conversion to type 2 diabetes: the Insulin Resistance Atherosclerosis Study (IRAS).},
   volume = {33},
   url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2928371&tool=pmcentrez&rendertype=abstract},
