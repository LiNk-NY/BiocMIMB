   issue = {7253},
   journal = {Nature},
   pages = {405-409},
   pmid = {19578362},
   publisher = {Nature Publishing Group},
   title = {The AP-1 transcription factor Batf controls T(H)17 differentiation.},
   volume = {460},
   url = {http://dx.doi.org/10.1038/nature08114},
   year = {2009},
}
