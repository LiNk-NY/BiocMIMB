   journal = {JAMA : the journal of the American Medical Association},
   keywords = {Adult,Blood Pressure,Cardiovascular Diseases,Cardiovascular Diseases: prevention & control,Cholesterol, LDL,Cholesterol, LDL: blood,Cross-Over Studies,Diet, Atherogenic,Dietary Carbohydrates,Dietary Fats, Unsaturated,Dietary Proteins,Fatty Acids, Monounsaturated,Female,Humans,Hypertension,Lipids,Lipids: blood,Male,Middle Aged},
   month = {11},
