   issue = {1},
   journal = {PloS one},
   pages = {e87044},
   pmid = {24475218},
   title = {Impact of measurement error on testing genetic association with quantitative traits.},
   volume = {9},
   url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3901720&tool=pmcentrez&rendertype=abstract},
   year = {2014},
}
