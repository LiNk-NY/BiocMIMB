   issn = {1471-2105},
   journal = {BMC bioinformatics},
   keywords = {Animals,Artificial Intelligence,Disease,Disease: genetics,Gene Expression Profiling,Genomics,Genomics: methods,Humans,Linear Models,Mice,Regression Analysis,Software},
   month = {1},
   pages = {5},
   pmid = {23323760},
   title = {Random generalized linear model: a highly accurate and interpretable ensemble predictor.},
   volume = {14},
   url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3645958&tool=pmcentrez&rendertype=abstract},
   year = {2013},
}
