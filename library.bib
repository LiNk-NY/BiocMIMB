   issue = {7606},
   journal = {Nature},
   pages = {259-62},
   pmid = {27279224},
   publisher = {Nature Publishing Group},
   title = {Universality of human microbial dynamics.},
   volume = {534},
   url = {http://www.nature.com/doifinder/10.1038/nature18301\nhttp://www.ncbi.nlm.nih.gov/pubmed/27279224\nhttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4902290},
