   issn = {0018-9294},
   issue = {5},
   journal = {IEEE transactions on bio-medical engineering},
   keywords = {Biological,Blood Glucose,Blood Glucose: drug effects,Blood Glucose: metabolism,Computer Simulation,Female,Glucose Tolerance Test,Glucose Tolerance Test: methods,Humans,Insulin,Insulin: metabolism,Insulin: pharmacology,Male,Middle Aged,Models,Nonlinear Dynamics,Reproducibility of Results},
