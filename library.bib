   month = {1},
   pages = {95-102},
   pmid = {21045073},
   title = {dmGWAS: dense module searching for genome-wide association studies in protein-protein interaction networks.},
   volume = {27},
   url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3008643&tool=pmcentrez&rendertype=abstract},
   year = {2011},
}
