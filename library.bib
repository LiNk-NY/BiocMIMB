   keywords = {Animals,Apoptosis,Biological,Cloning,Erythroid Precursor Cells,Erythroid Precursor Cells: metabolism,Erythropoietin,Erythropoietin: pharmacology,Female,In Situ Nick-End Labeling,Inbred BALB C,Janus Kinase 2,Janus Kinase 2: genetics,Janus Kinase 2: metabolism,Ligands,Messenger,Mice,Microarray Analysis,Models,Molecular,Phosphorylation,RNA,Reverse Transcriptase Polymerase Chain Reaction,STAT5 Transcription Factor,STAT5 Transcription Factor: genetics,STAT5 Transcription Factor: metabolism,Signal Transduction,Suppressor of Cytokine Signaling Proteins,Suppressor of Cytokine Signaling Proteins: genetic,Suppressor of Cytokine Signaling Proteins: metabol},
   pages = {516-516},
