   issn = {00219738},
   issue = {6},
   journal = {Journal of Clinical Investigation},
   pages = {2604-2615},
   pmid = {23676462},
   title = {PD-L1 blockade synergizes with IL-2 therapy in reinvigorating exhausted T cells},
   volume = {123},
   year = {2013},
}
